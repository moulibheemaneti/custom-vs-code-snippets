{
	"Media Query Mobile Only SCSS Mixin": {
		"prefix": "mobile-only-mq",
		"body": [
			"@include mobile {",
			"    $1",
			"}"
		]
	},
	"Media Query Desktop Only SCSS Mixin": {
		"prefix": "desktop-only-mq",
		"body": [
			"@include desktop {",
			"    $1",
			"}"
		]
	},
	"Media Query Web Screen Only SCSS Mixin": {
		"prefix": "web-screen-only-mq",
		"body": [
			"@include web-screen {",
			"    $1",
			"}"
		]
	},
	"Media Query Tab Screen Only SCSS Mixin": {
		"prefix": "tab-screen-only-mq",
		"body": [
			"@include tab-screen {",
			"    $1",
			"}"
		]
	},
	"Media Query Large Screen Only SCSS Mixin": {
		"prefix": "large-screen-only-mq",
		"body": [
			"@include large-screen {",
			"    $1",
			"}"
		]
	},
	"Create Mixin with cursor at mixin-name": {
		"prefix": "mixin-name",
		"body": [
			"@mixin $1 {",
			"    $2",
			"}"
		]
	},
	"Create Include with cursor at include-name": {
		"prefix": "include-name",
		"body": [
			"@include $1;"
		]
	},
	"REM function in scss": {
		"prefix": "rem",
		"body": [
			"rem($1)"
		]
	},
	"Create CSS class with cursor at css-name": {
		"prefix": "css-name",
		"body": [
			".$1 {",
			"    $2",
			"}"
		]
	}
}
